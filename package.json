{
  "name": "coffee",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "pretty-quick": "^3.1.3",
    "turbo": "^1.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@9.2.0"
}
